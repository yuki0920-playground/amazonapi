<% if @products.present? %>
  <% @products.each do |product| %>
    <div class="list">
      <%= link_to (image_tag(product.image_url, style: "width: 70px;height: 100px;display: inline-block;")), product.url %>
      <p><%= product.title %></p>
      <p><%= product.asin %></p>
    </div>
  <% end %>
<% end %>